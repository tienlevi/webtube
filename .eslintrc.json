{
  "extends": [
    "next/core-web-vitals",
    "next/next/no-img-element",
    "jsx-a11y/alt-text"
  ],
  "rules": {
    "jsx-a11y/alt-text": [
      2,
      {
        "elements": ["img", "object", "area", "input[type=\"image\"]"],
        "img": ["Image"],
        "object": ["Object"],
        "area": ["Area"],
        "input[type=\"image\"]": ["InputImage"]
      }
    ]
  }
}
